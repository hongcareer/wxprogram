<!--pages/template/temp.wxml-->
<template name="temp">
  <view class="tempContainer">
    <view class="headerContainer">
      <image src="{{avatar}}"></image>
      <text>{{date}}</text>
    </view>
    <text class="title">{{title}}</text>
    <image src="{{headImgSrc}}" class="mainPic"></image>
    <text class="mainTxt">{{detail_content}}</text>
    <view class="footContainer">
      <image src="{{detail_love_image1}}"></image>
      <text>{{love_count}}</text>
      <image src="{{detail_love_image2}}"></image>
      <text>{{attention_count}}</text>
    </view>
  </view>
</template>
